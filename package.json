{
  "name": "@advanced-rest-client/web-url-input",
  "version": "2.0.0",
  "license": "Apache-2.0",
  "description": "An element to display a dialog to enter an URL with auto hints",
  "devDependencies": {
    "@polymer/gen-typescript-declarations": "^1.6.0"
  },
  "private": false,
  "main": "web-url-input.html",
  "scripts": {
    "lint": "polymer lint web-url-input.html",
    "test-sauce": "polymer test --plugin sauce --job-name \"web-url-input:local-test\"",
    "test": "polymer test --plugin local",
    "update-types": "gen-typescript-declarations --deleteExisting --outDir .",
    "analysis": "polymer analyze -i web-url-input.html > analysis.json"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/advanced-rest-client/web-url-input.git"
  },
  "bugs": {
    "url": "https://github.com/advanced-rest-client/web-url-input/issues",
